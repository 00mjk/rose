# do not edit this file

FLAGS = -I /usr/local/mstools/include/rose$(ROSEVERSION) -I $(PAGDIR)/include/pag -I $(PAGDIR)/include/pag/analyzer -I ../astaccess
OBJS = CFGTraversal.o ExprLabeler.o ExprTransformer.o ProcTraversal.o AstNumberOfNodesStatistics.o cfg_funcs.o cfg_support.o testcfg.o

all: libsatire_rose_icfg.a

libsatire_rose_icfg.a: $(OBJS)
	ar -csr libsatire_rose_icfg.a $(OBJS)

testcfg.o: testcfg.c testcfg.h
	gcc $(FLAGS) -c testcfg.c

%.o: %.C CFGTraversal.h cfg_support.h
	g++ $(FLAGS) -c $<

.PHONY: clean
clean:
	rm -f *~ *.o rosie *.dot *.ps *.a *.gdl
