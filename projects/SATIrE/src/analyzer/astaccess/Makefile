# do not edit this file

FLAGS = -I $(ROSE_INCLUDE) -I $(PAGDIR)/include/pag -I $(PAGDIR)/include/pag/analyzer -I ../icfgbuilder -I .
all: libastaccess.a

libastaccess.a: syntree.o pag_support.o 
	ar -csr libastaccess.a syntree.o pag_support.o 

syntree.h syntree.c: syn rose.pig
	$(PIG) -ip syn rose.pig

syntree.o: syntree.h syntree.c
	g++ $(FLAGS) -c syntree.c

pag_support.o: pag_support.c pag_support.h
	g++ $(FLAGS) -c pag_support.c

.PHONY: clean
clean:
	rm -f *~ *.a *.o pig_temp* syntree*
