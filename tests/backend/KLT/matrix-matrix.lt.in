params(n,m,p)
data(A[0:n][0:p], flow-in)
data(B[0:p][0:m], flow-in)
data(C[0:n][0:m], flow-out)
loop-trees(
  loop(
    i, 0, n-1, parfor,
    loop(
      j, 0, m-1, parfor,
      stmt(C[i][j]=0;),
      loop(
        k, 0, p-1, reduction(C[i][j]),
        stmt(C[i][j]+=A[i][k]*B[k][j];)
      )
    )
  )
)
